{
  "name": "first-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npx @tailwindcss/cli -i ./src/style.css -o  ./src/main.css --watch\" \"vite\"",
    "build": "tsc && vite build",
